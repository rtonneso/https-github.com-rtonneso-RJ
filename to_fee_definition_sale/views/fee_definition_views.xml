<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record model="ir.ui.view" id="fee_definition_form_view">
			<field name="name">fee.definition.form</field>
			<field name="model">fee.definition</field>
			<field name="inherit_id"
				ref="to_fee_definition.fee_definition_form_view" />
			<field name="arch" type="xml">
				<xpath expr="//field[@name='uom_id']" position="after">
					<field name="lst_price" />
					<field name="sale_price" />
				</xpath>
			</field>
		</record>

		<record model="ir.ui.view" id="fee_definition_tree_view">
			<field name="name">fee.definition.tree</field>
			<field name="model">fee.definition</field>
			<field name="inherit_id"
				ref="to_fee_definition.fee_definition_tree_view" />
			<field name="arch" type="xml">
				<xpath expr="//field[@name='uom_id']" position="after">
					<field name="lst_price" />
					<field name="sale_price" />
				</xpath>
			</field>
		</record>
	</data>
</odoo>