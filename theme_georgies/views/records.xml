  <!-- Set various ui components to active by default -->
  <!-- Shop -->
  <!-- Shop categories filter -->
  <record id="website_sale.products_categories" model="ir.ui.view">
    <field name="active" eval="True"/>
  </record>
  <!-- Shop collapsable categories -->
  <record id="website_sale.option_collapse_products_categories" model="ir.ui.view">
    <field name="active" eval="True"/>
  </record>
  <!-- Shop attribute filters -->
  <record id="website_sale.products_attributes" model="ir.ui.view">
    <field name="active" eval="True"/>
  </record>
  <!-- Shop sort button -->
  <record id="website_sale.sort" model="ir.ui.view">
    <field name="active" eval="True"/>
  </record>
  <!-- Shop results layout button -->
  <record id="website_sale.add_grid_or_list_option" model="ir.ui.view">
    <field name="active" eval="True"/>
  </record>

  <!-- Product Description -->
  <record id="website_sale.products_description" model="ir.ui.view">
    <field name="active" eval="False"/>
  </record>
  <!-- Add to Cart button -->
  <record id="website_sale.products_add_to_cart" model="ir.ui.view">
    <field name="active" eval="True"/>
  </record>
  <!-- Disable default Terms and Conditions section on the product page. It is the same across all products and therefore isn't all that useful. -->
  <record id="website_sale.product_custom_text" model="ir.ui.view">
    <field name="active" eval="False"/>
  </record>


  <!-- Product -->
  <!-- Buy now button -->
  <record id="website_sale.product_buy_now" model="ir.ui.view">
    <field name="active" eval="True"/>
  </record>
  <!-- Product image magnification -->
  <record id="website_sale.product_picture_magnify_auto" model="ir.ui.view">
    <field name="active" eval="True"/>
  </record>

  <!-- Blog -->
  <!-- Show sidebar on blog index -->
  <record id="website_blog.opt_blog_sidebar_show" model="ir.ui.view">
    <field name="active" eval="True"/>
  </record>
  <!-- Show comment and view stats on blog posts -->
  <record id="website_blog.opt_posts_loop_show_stats" model="ir.ui.view">
    <field name="active" eval="True"/>
  </record>
  <!-- Enable blog post comments -->
  <record id="website_blog.opt_blog_post_comment" model="ir.ui.view">
    <field name="active" eval="True"/>
  </record>
  <!-- Show blog post sidebar -->
  <record id="website_blog.opt_blog_post_sidebar" model="ir.ui.view">
    <field name="active" eval="True"/>
  </record>



